#include"stdio.h"
#include"conio.h"
#include"math.h"

void main()
{
	float x, f, y;
	printf("Vvedite x\n");
	scanf_s("%f", &x);
	printf("Vvedite y\n");
	scanf_s("%f", &y);
	f = 0.8*((sin((x - y) / 2) + cos((x - y) / 2)) / (sin(x)*sin(x) + exp(x)) + sqrt(x + 2));
	printf("f = %f", f);
	_getch();
}




#include"stdio.h"
#include"conio.h"
#include"math.h"

void main()
{
	float x, y, a, b, c, d, f;
	printf("Vvedite x\n");
	scanf_s("%f", &x);
	printf("Vvedite y\n");
	scanf_s("%f", &y);
	printf("Vvedite a\n");
	scanf_s("%f", &a);	
	printf("Vvedite b\n");
	scanf_s("%f", &b);	
	printf("Vvedite c\n");
	scanf_s("%f", &c);	
	printf("Vvedite d\n");
	scanf_s("%f", &d);
	f = sqrt((a + b*c + 3 * d) / (2 + x*y))*sin(y);
	printf("f = %f", f);
	_getch();
}







4
#include"stdio.h"
#include"conio.h"
#include"math.h"

int main()
{
	float x, f, y;
	printf("Vvedite x\n");
	scanf("%f", &x);
	printf("Vvedite y\n");
	scanf("%f", &y);
	f = 0.8*((sin((x - y) / 2) + cos((x - y) / 2)) / (sin(x)*sin(x) + exp(x)) + sqrt(x + 2));
	printf("f = %f", f);
	getch();
}






